{% extends('::layout/layout.html.twig') %}

{% block body -%}
    <h1>Reponse</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Textereponse</th>
                <td>{{ entity.texteReponse }}</td>
            </tr>
            <tr>
                <th>Dateajoutreponse</th>
                <td>{{ entity.dateAjoutReponse|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('reponse') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reponse_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
