{% extends('::layout/layout_admin.html.twig') %}

{% block body -%}
    <h1>Budget</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombudget</th>
                <td>{{ entity.nomBudget }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_budget') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_budget_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
